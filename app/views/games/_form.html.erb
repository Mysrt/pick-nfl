<%= form_for @game do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :week_id %><br />
    <%= f.select :week_id, (1..16).to_a %>
  </p>
  <p>
    <%= f.label :home_team_score %><br />
    <%= f.text_field :home_team_score %>
  </p>

  <p>
    <%= f.label :away_team_score %><br />
    <%= f.text_field :away_team_score %>
  </p>
  <p>
    <%= f.label :home_team_id %><br />
    <%= f.select :home_team_id, FootballTeam.teams_hash %>
  </p>
  <p>
    <%= f.label :away_team_id %><br />
    <%= f.select :away_team_id, FootballTeam.teams_hash %>
  </p>
<%= FootballTeam.teams_hash.inspect %>
  <p><%= f.submit %></p>

<% end %>
